<h1>Jonas' Calculator</h1>

<div class="calculator">
  <div class="display">
    <%= Calculator.display(:calculator) %>
  </div>
  <div class="keypad">
    <%= for key <- ~w/1 2 3 +  4 5 6 - 7 8 9 : 0 . = */ do %>
      <div class="key"><%=
        link(key, to: "/key/#{key}") %></div>
    <% end  %>
  </div>
</div>